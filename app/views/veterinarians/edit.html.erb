<h1>Edit Veterinarian</h1>

<div class="container-sm mx-5">
  <%= form_with url: "/veterinarians/#{@veterinarian.id}", method: :patch, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :on_call %>
    <%= f.check_box :on_call %>

    <%= f.label :review_rating %>
    <%= f.number_field :review_rating %>

    <%= f.submit %>
  <% end %>
</div>
